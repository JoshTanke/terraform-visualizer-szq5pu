{
  "name": "terraform-visualizer-backend",
  "version": "1.0.0",
  "description": "Backend services for Terraform Visualization Tool",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "security:audit": "npm audit",
    "docker:build": "docker build -t terraform-visualizer-backend .",
    "docker:run": "docker run -p 3000:3000 terraform-visualizer-backend",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hashicorp/hcl2-parser": "^1.0.0",
    "@octokit/plugin-retry": "^4.0.0",
    "@octokit/plugin-throttling": "^5.0.0",
    "@octokit/rest": "^19.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3-force": "^3.0.0",
    "dagre": "^1.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.0",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "opossum": "^6.0.0",
    "pino": "^8.0.0",
    "rate-limit-redis": "^3.0.0",
    "redis": "^4.6.0",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.6.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.13.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "ioredis-mock": "^8.0.0",
    "jest": "^29.0.0",
    "jest-mock": "^29.0.0",
    "jest-mock-express": "^0.2.2",
    "lint-staged": "^13.0.0",
    "nock": "^13.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}